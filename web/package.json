{
  "name": "you-are-a-failure",
  "description": "The App that will keep reminding you that \"you are a failure!\"",
  "version": "1.0.0",
  "type": "module",
  "author": "Leomotors",
  "repository": "https://github.com/Leomotors/you-are-a-failure",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build && node postbuild.js",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"*.config.{cjs,js,ts}\" && prettier --check \"src/**/*.{ts,svelte,html}\" && eslint src && eslint *.{cjs,js}",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" && prettier --write \"src/**/*.{ts,svelte,html}\""
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.311",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "globby": "^13.1.1",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.6.0",
    "svelte-preprocess": "^4.10.5",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  }
}
